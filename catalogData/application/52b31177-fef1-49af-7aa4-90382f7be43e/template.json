{
  "id": "52b31177-fef1-49af-7aa4-90382f7be43e",
  "body": {
    "componentType": "instance",
    "persistentVolumeClaims": null,
    "deployments": [
      {
        "kind": "Deployment",
        "apiVersion": "extensions/v1beta1",
        "metadata": {
          "name": "$idx_and_short_instance_id",
          "creationTimestamp": null,
          "labels": {
            "idx_and_short_instance_id": "$idx_and_short_instance_id",
            "instance_id": "$instance_id",
            "managed_by": "TAP",
            "org": "$org",
            "space": "$space"
          }
        },
        "spec": {
          "replicas": 1,
          "selector": {
            "matchLabels": {
              "idx_and_short_instance_id": "$idx_and_short_instance_id",
              "instance_id": "$instance_id"
            }
          },
          "template": {
            "metadata": {
              "creationTimestamp": null,
              "labels": {
                "idx_and_short_instance_id": "$idx_and_short_instance_id",
                "instance_id": "$instance_id",
                "managed_by": "TAP"
              }
            },
            "spec": {
              "volumes": [
                {
                  "name": "$idx_and_short_instance_id-ca-pem-volume",
                  "secret": {
                    "secretName": "platform-ca-root-cert"
                  }
                },
                {
                  "name": "$idx_and_short_instance_id-ca-certificates-crt-volume",
                  "secret": {
                    "secretName": "platform-ca-roots-bundle"
                  }
                },
                {
                  "name": "$idx_and_short_instance_id-hashed-ca-volume",
                  "secret": {
                    "secretName": "platform-ca-root-cert-hashed"
                  }
                },
                {
                  "name": "$idx_and_short_instance_id-certkey-volume",
                  "secret": {
                    "secretName": "$idx_and_short_instance_id-certkey"
                  }
                }
              ],
              "containers": [
                {
                  "name": "app",
                  "image": "$image",
                  "volumeMounts": [
                    {
                      "name": "$idx_and_short_instance_id-ca-pem-volume",
                      "mountPath": "/etc/ssl/certs/ca.pem",
                      "subPath": "ca.pem",
                      "readOnly": true
                    },
                    {
                      "name": "$idx_and_short_instance_id-ca-certificates-crt-volume",
                      "mountPath": "/etc/ssl/certs/ca-certificates.crt",
                      "subPath": "ca-certificates.crt",
                      "readOnly": true
                    },
                    {
                      "name": "$idx_and_short_instance_id-hashed-ca-volume",
                      "mountPath": "/etc/ssl/certs/$cert_hash.0",
                      "subPath": "$cert_hash.0",
                      "readOnly": true
                    }
                  ],
                  "ports": [
                    {
                      "containerPort": 80,
                      "protocol": "TCP"
                    }
                  ],
                  "env": [
                    {
                      "name": "VERSION",
                      "value": "1.0"
                    },
                    {
                      "name": "MANAGED_BY",
                      "value": "TAP"
                    },
                    {
                      "value": "80",
                      "name": "PORT"
                    }
                  ],
                  "resources": {
                    "limits": {
                      "memory": "$memory_limit"
                    }
                  },
                  "imagePullPolicy": "IfNotPresent"
                },
                {
                  "name": "nginx-ssl",
                  "image": "127.0.0.1:30000/$nginx_ssl_name",
                  "volumeMounts": [
                    {
                      "name": "$idx_and_short_instance_id-certkey-volume",
                      "mountPath": "/etc/nginx/ssl"
                    }
                  ],
                  "ports": [
                    {
                      "containerPort": 443,
                      "protocol": "TCP"
                    }
                  ],
                  "imagePullPolicy": "Always"
                }
              ],
              "restartPolicy": "Always",
              "dnsPolicy": "ClusterFirst",
              "serviceAccountName": ""
            }
          },
          "status": {}
        }
      }
    ],
    "ingresses": [
      {
        "kind": "Ingress",
        "apiVersion": "extensions/v1beta1",
        "metadata": {
          "name": "$idx_and_short_instance_id",
          "creationTimestamp": null,
          "labels": {
            "idx_and_short_instance_id": "$idx_and_short_instance_id",
            "instance_id": "$instance_id",
            "managed_by": "TAP"
          },
          "annotations": {
            "useSsl": "true"
          }
        },
        "spec": {
          "rules": [
            {
              "host": "$hostname.$domain_name",
              "http": {
                "paths": [
                  {
                    "path": "/",
                    "backend": {
                      "serviceName": "$idx_and_short_instance_id",
                      "servicePort": 443
                    }
                  }
                ]
              }
            }
          ]
        },
        "status": {
          "loadBalancer": {}
        }
      }
    ],
    "services": [
      {
        "kind": "Service",
        "apiVersion": "v1",
        "metadata": {
          "name": "$idx_and_short_instance_id",
          "creationTimestamp": null,
          "labels": {
            "idx_and_short_instance_id": "$idx_and_short_instance_id",
            "instance_id": "$instance_id",
            "managed_by": "TAP",
            "org": "$org",
            "space": "$space"
          }
        },
        "spec": {
          "type": "NodePort",
          "ports": [
            {
              "name": "rest",
              "protocol": "TCP",
              "port": 80,
              "targetPort": 0,
              "nodePort": 0
            },
            {
              "name": "rest-ssl",
              "protocol": "TCP",
              "port": 443,
              "targetPort": 0,
              "nodePort": 0
            }
          ],
          "selector": {
            "instance_id": "$instance_id"
          }
        },
        "status": {
          "loadBalancer": {}
        }
      }
    ],
    "serviceAccounts": null,
    "secrets": null
  },
  "hooks": null
}
