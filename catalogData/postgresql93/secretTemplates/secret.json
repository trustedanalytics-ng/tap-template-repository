{
  "kind": "Secret",
  "apiVersion": "v1",
  "metadata": {
    "name": "$short_serviceid-postgresql93-credentials",
    "labels": {
      "service_id": "$service_id",
      "idx_and_short_serviceid": "$idx_and_short_serviceid",
      "managed_by": "TAP"
    }
  },
  "data": {
    "postgres-username": "$base64-$random1",
    "postgres-password": "$base64-$random2",
    "postgres-dbname": "$base64-$random3"
  }
}