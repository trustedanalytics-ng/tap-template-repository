{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "$idx_and_short_serviceid-node0",
    "labels": {
      "org": "$org",
      "space": "$space",
      "catalog_service_id": "$catalog_service_id",
      "catalog_plan_id": "$catalog_plan_id",
      "service_id": "$service_id",
      "idx_and_short_serviceid": "$idx_and_short_serviceid-node0",
      "managed_by": "TAP",
      "instance": "node0"
    }
  },
  "spec": {
    "type": "NodePort",
    "selector": {
      "service_id": "$service_id",
      "idx_and_short_serviceid": "$idx_and_short_serviceid-node0"
    },
    "ports": [
      {
        "port": 27017,
        "targetPort": 27017,
        "protocol": "TCP"
      }
    ]
  }
}
