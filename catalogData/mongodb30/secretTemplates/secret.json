{
  "kind": "Secret",
  "apiVersion": "v1",
  "metadata": {
    "name": "$short_serviceid-mongodb30-simple-secret-keyfile",
    "labels": {
      "service_id": "$service_id",
      "idx_and_short_serviceid": "$idx_and_short_serviceid",
      "managed_by": "TAP"
    }
  },
  "data": {
    "mongodb-username": "$base64-$random2",
    "mongodb-password": "$base64-$random3",
    "mongodb-dbname": "$base64-$random1"
  }
}