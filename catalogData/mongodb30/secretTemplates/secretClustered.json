{
  "kind": "Secret",
  "apiVersion": "v1",
  "metadata": {
    "name": "$short_serviceid-mongodb30-clustered-secret-keyfile",
    "labels": {
      "service_id": "$service_id",
      "idx_and_short_serviceid": "$idx_and_short_serviceid",
      "managed_by": "TAP"
    }
  },
  "data": {
    "keyfile": "$base64-$random1",
    "mongodb-username": "$base64-$random2",
    "mongodb-password": "$base64-$random3",
    "mongodb-dbname": "$base64-admin",
    "mongodb-replica-name": "$base64-replica",
    "mongodb-role": "$base64-root"
  }
}