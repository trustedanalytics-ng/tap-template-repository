    "name": "$short_serviceid-trustedanalytics-mosquitto-credentials",
    "labels": {
      "service_id": "$service_id",
      "idx_and_short_serviceid": "$idx_and_short_serviceid",
      "managed_by": "TAP"
    }
  },
  "data": {
    "mosquitto-password": "$base64-$random1",
