{
  "kind": "Secret",
  "apiVersion": "v1",
  "metadata":{
    "name":"postgressecret.$secret_idx_and_short_serviceid",
    "labels": {
      "secretClass":"postgres",
      "managed_by": "TAP",
      "name":"$name-secret"
    }
  },
  "type":"Opaque",
  "data":{
    "postgresuser": "$base64-$random1",
    "postgrespassword": "$base64-$random2",
    "postgresdbname": "$base64-$random3",
    "postgresuri":""
  }
}